{
  "coc.preferences.formatOnSaveFiletypes": [
    "css",
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact",
    "json",
    "graphql"
  ],
  "python.formatting.provider": "black",
  "python.formatting.blackArgs": ["--line-length", "120"],
  "python.linting.flake8Enabled": true,
  "python.linting.flake8Args": ["--max-line-length", "120"]
}
